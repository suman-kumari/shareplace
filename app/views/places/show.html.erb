<div>
  <div class="pull-right">
    <div class="dropdown">
      <button class="btn btn-primary">Share My Place</button>
       <ul class="dropdown-menu">
        <li><%= link_to 'Public', share_place_path(@place) %></li>
        <li><%= link_to 'Selected People', new_user_share_place_path(current_user, place_id: params[:id]) %></li>
       </ul>
    </div>
    <%= link_to 'Back', places_path, class: 'btn btn-primary' %>
  </div>
  <h1><%= @place.title %></h1>
</div>

<p>Address: <%= @place.address %></p>
<p>Coordinates: <%= @place.latitude %> <%= @place.longitude %></p>
<%= image_tag google_map(@place), class: 'img-fluid img-rounded', alt: "#{@place.title} on the map"%>
